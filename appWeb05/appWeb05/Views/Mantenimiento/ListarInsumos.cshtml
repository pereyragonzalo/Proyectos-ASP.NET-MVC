@model IEnumerable<appWeb05.Models.Insumos>

@{
    ViewBag.Title = "Listar Insumos";
}

<h2>Listar insumos</h2>

<p>
    @Html.ActionLink("Nuevo","Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.idInsumo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nomInsumo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomProveedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.preUnitario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.stockUnitario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.idInsumo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nomInsumo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomProveedor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.preUnitario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.stockUnitario)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.idInsumo }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.idInsumo }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.idInsumo })
        </td>
    </tr>
}
</table>
