@model IEnumerable<webAppT1.Models.PedidosEmpleado>

@{
    ViewBag.Title = "consultaPedido2";
}

<h2>consultaPedido2</h2>
<br />

<div>
    @using (@Html.BeginForm())
    {
        <en>Desde : </en>
        @Html.TextBox("fec1", "", new { type = "date" })
        <en>Hasta : </en>
        @Html.TextBox("fec2", "", new { type = "date" })
        <button>Consultar</button>
    }
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.order_id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.order_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.last_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.first_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.freight)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ship_address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ship_city)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.order_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.order_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.last_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.first_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.freight)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ship_address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ship_city)
            </td>
        </tr>
    }

</table>
